<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.piotrek.listwa.StanListwy">

      <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Stan listwy"
        android:id="@+id/textView8"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:background="#FF0000"
        android:textColor="#FFF"
        android:textSize="18dp"
        android:text="Połączono z Internetem?"
        android:id="@+id/isConnected"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/textView8"
        android:layout_centerHorizontal="true" />




    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D5  "
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/switchD5"
        android:layout_marginTop="30dp"
        android:checked="false"
        android:layout_below="@+id/isConnected"
        android:layout_centerHorizontal="true" />

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D16 "
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/switchD16"
        android:layout_marginTop="15dp"
        android:checked="false"
        android:layout_below="@+id/switchD5"
        android:layout_centerHorizontal="true" />

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D14 "
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/switchD14"
        android:layout_marginTop="15dp"
        android:checked="false"
        android:layout_below="@+id/switchD16"
        android:layout_centerHorizontal="true" />

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D12 "
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/switchD12"
        android:layout_marginTop="15dp"
        android:checked="false"
        android:layout_below="@+id/switchD14"
        android:layout_centerHorizontal="true" />

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D13 "
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/switchD13"
        android:layout_marginTop="15dp"
        android:checked="false"
        android:layout_below="@+id/switchD12"
        android:layout_centerHorizontal="true" />






</RelativeLayout>
